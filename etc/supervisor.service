[Unit]
Description=NearAI Supervisor
After=network.target

[Service]
User=setup
Type=simple
Restart=no
WorkingDirectory=/home/setup/.nearai/nearai-cli
EnvironmentFile=/home/setup/.nearai/nearai-cli/etc/supervisor.env
ExecStart=/home/setup/.nearai/nearai-cli/etc/start_supervisor.sh
LimitMEMLOCK=infinity
LimitRSS=infinity

[Install]
WantedBy=multi-user.target
